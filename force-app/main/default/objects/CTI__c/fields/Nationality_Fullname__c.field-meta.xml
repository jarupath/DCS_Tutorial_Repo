<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Nationality_Fullname__c</fullName>
    <description>Passenger's nationality in fullname format with Unidentify for blank value.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(TEXT(Nationality_txt__c))),TEXT(Nationality_txt__c),"Unidentified")</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Nationality</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
