<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Accepted_Seat__c</fullName>
    <description>Display Passenger's Seat (post check-in) from user input. Formula will default Unidentified value for blank Accepted Seat.</description>
    <externalId>false</externalId>
    <formula>IF( ISBLANK(Accepted_Seat_txt__c) || Accepted_Seat_txt__c == "NA","Unidentified" , Accepted_Seat_txt__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Accepted Seat</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
