<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ActiveStatus__c</fullName>
    <description>Displays the icon active or inactive for the Case Teams</description>
    <externalId>false</externalId>
    <formula>IMAGE("/resource/icon_case/"&amp;IF(Active__c == true,"Status_Acknowledge.png","Status_Inactive.png"),"",24,24)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
