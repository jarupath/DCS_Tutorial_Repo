<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsFirst_Platinum__c</fullName>
    <description>Displays whether the passenger related to the case is a first class passenger or a platinum tier passenger</description>
    <externalId>false</externalId>
    <formula>IF( Accepted_Class__c == "F" || ROP_Tier__c == "PLAT" , true , false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>IsFirst_Platinum</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
