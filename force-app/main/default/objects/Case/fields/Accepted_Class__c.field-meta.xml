<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Accepted_Class__c</fullName>
    <description>Displays the value of the Passenger's Travel Class if available (post check-in)</description>
    <externalId>false</externalId>
    <formula>if(Accepted_Class_txt__c != null,Accepted_Class_txt__c,IF(Passengerid__r.Cabin_Code__c != null,Passengerid__r.Cabin_Code__c,"Unidentified"))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Accepted Class</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
